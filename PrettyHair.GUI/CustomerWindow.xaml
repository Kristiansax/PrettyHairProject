<Window x:Class="PrettyHair.GUI.CustomerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PrettyHair.GUI"
        mc:Ignorable="d"
        Title="CustomerWindow" Height="276.259" Width="489.928">
    <Grid Margin="0,0,3.6,3">
        <TextBox x:Name="FirstNameTextBox" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="160" Margin="70,112,0,0"/>
        <TextBox x:Name="LastNameTextBox" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="160" Margin="256,112,0,0"/>
        <Label x:Name="FirstNameLabel" Content="First Name" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="70,69,0,0"/>
        <Label x:Name="LastNameLabel" Content="Last Name" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="256,69,0,0"/>
        <Button x:Name="CreateButton" Content="Create" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="247,213,0,0" Click="CreateButton_Click"/>
        <Button x:Name="CancelButton" Content="Cancel" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="341,213,0,0" Click="CancelButton_Click"/>

    </Grid>
</Window>
